    <script>tinymce.init({ selector: 'textarea' });</script>
    <style>
        .fas {
            font-size: 15px;
            margin-right: 2px;
        }
        .btn-email {
            width: 110px;
        }
    </style>

</head>

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">

        <!-- Sidebar - Brand -->
